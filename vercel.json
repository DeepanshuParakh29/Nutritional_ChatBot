{
  "version": 2,
  "builds": [
    { "src": "api/chat.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/chat", "dest": "api/chat.py" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "functions": {
    "api/chat.py": {
      "runtime": "python3.9"
    }
  }
}
